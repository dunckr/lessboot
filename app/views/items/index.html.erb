
<header class="navbar navbar-static-top">
  <div class="container">
    <div class="pull-right">
      dunckr
    </div>
    <h3 class="">LessBoot</h3>
  </div>
</header>

<div class="container">
  <div class="row">
    <% @items.each do |item| %>
      <div class="col-lg-12 centered">
        <div id="masonry-container" class="transitions-enabled">
          <div class="box">
            <p>
              <a href=<%= link_to 'Show', item %>
                <img src=<%= item.live_preview_url %> />
              </a>
            </p>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>

<footer>
  <div class="container">
    <div class="footer center-block">
      <%= will_paginate @items %>
    </div>
  </div>
</footer>
